    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms", 
        "craft-plugin",
        "formie",
        "rest api",
        "graphql",
        "api"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "LindemannRock",
            "homepage": "https://lindemannrock.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "craftcms/cms": "^5.0.0",
        "verbb/formie": "^3.0.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "lindemannrock\\formierestapi\\": "src/"
        }
    },
    "extra": {
        "name": "Formie REST API",
        "handle": "formie-rest-api",
        "developer": "LindemannRock",
        "developerUrl": "https://lindemannrock.com",
        "documentationUrl": "https://github.com/lindemannrock/formie-rest-api/blob/master/README.md",
        "changelogUrl": "https://github.com/lindemannrock/formie-rest-api/blob/master/CHANGELOG.md",
        "class": "lindemannrock\\formierestapi\\FormieRestApi"
    }
