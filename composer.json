{
	"name": "lindemannrock/craft-formie-rest-api",
	"description": "REST and GraphQL API for Formie - Provides API endpoints for accessing Formie forms and submissions",
	"type": "craft-plugin",
	"version": "3.1.0",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"formie",
		"rest api",
		"graphql",
		"api"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Bilal Hary Lindemann",
			"email": "bh@lindemannrock.com",
			"homepage": "https://lindemannrock.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "support@lindemannrock.com",
		"issues": "https://github.com/LindemannRock/craft-formie-rest-api/issues",
		"source": "https://github.com/LindemannRock/craft-formie-rest-api",
		"docs": "https://github.com/LindemannRock/craft-formie-rest-api/blob/main/README.md",
		"rss": "https://github.com/LindemannRock/craft-formie-rest-api/commits/main.atom"
	},
	"require": {
		"php": "^8.2",
		"craftcms/cms": "^5.0.0",
		"verbb/formie": "^3.0.0",
		"guzzlehttp/guzzle": "^7.0",
		"lindemannrock/craft-plugin-base": "^5.0"
	},
	"require-dev": {
		"craftcms/ecs": "dev-main",
		"craftcms/phpstan": "dev-main"
	},
	"autoload": {
		"psr-4": {
			"lindemannrock\\formierestapi\\": "src/"
		}
	},
	"scripts": {
		"phpstan": "phpstan --ansi --memory-limit=1G",
		"check-cs": "ecs check --ansi",
		"fix-cs": "ecs check --fix --ansi"
	},
	"extra": {
		"name": "Formie REST API",
		"handle": "formie-rest-api",
		"developer": "LindemannRock",
		"developerUrl": "https://lindemannrock.com",
		"documentationUrl": "https://github.com/LindemannRock/craft-formie-rest-api/blob/main/README.md",
		"changelogUrl": "https://github.com/LindemannRock/craft-formie-rest-api/blob/main/CHANGELOG.md",
		"class": "lindemannrock\\formierestapi\\FormieRestApi"
	}
}
